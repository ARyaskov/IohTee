  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ARyaskov/iohtee.git"
  },
  "engines": {
    "node": ">=20"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": "dist/bin/iohtee-abi-wrapper.js",
  "files": [
    "dist/",
    "templates/"
  ],
  "scripts": {
    "demo": "tsx src/bin/iohtee-abi-wrapper.ts ./abi/* -m -d ./docs -o ../contracts/src/abi-wrapper/",
    "build": "yarn format && yarn clean && tsc --project tsconfig.json",
    "clean": "rimraf dist",
    "test": "exit 0",
    "coverage": "exit 0",
    "format": "npx prettier --write \"src/**/*.ts\"",
    "postinstall": "exit 0"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "console.table": "^0.10.0",
    "esbuild": "^0.21.5",
    "fs-extra": "^11.2.0",
    "glob": "^10.4.2",
    "handlebars": "^4.7.8",
    "mkdirp": "^3.0.1",
    "viem": "*",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/yargs": "^17.0.32",
    "rimraf": "*",
    "ts-node": "^10.9.2",
    "typedoc": "^0.26.2",
    "typedoc-plugin-markdown": "^4.1.0",
    "typescript": "*"
  }
